<ivy-module version="2.0">
    <info organisation="kairosdb.org" module="kairos-rabbitmq"/>
    <configurations defaultconf="default" >
        <conf name="default"/>
    </configurations>
    <dependencies>
        <!-- Basic dependencies (already included in KairosDB) -->
        <dependency org="com.google.inject" name="guice" rev="3.0" />
        <dependency org="org.slf4j" name="jul-to-slf4j" rev="1.7.2" />
        <dependency org="org.slf4j" name="slf4j-api" rev="1.7.20" />
        <dependency org="org.apache.commons" name="commons-lang3" rev="3.4" />
        <dependency org="commons-io" name="commons-io" rev="2.5" />
        <dependency org="org.json" name="org.json" rev="chargebee-1.0" />
        <dependency org="com.google.guava" name="guava" rev="19.0" />
        <dependency org="javax.inject" name="javax.inject" rev="1" />
        <!-- Custom dependencies -->
        <dependency org="com.rabbitmq" name="amqp-client" rev="3.6.6" />
        <dependency org="org.springframework.amqp" name="spring-amqp" rev="1.6.5.RELEASE" />
        <dependency org="org.reflections" name="reflections" rev="0.9.10"/>
        <!-- KairosDB -->
        <dependency org="org.kairosdb" name="kairosdb" rev="1.1.3-1" />
    </dependencies>
</ivy-module>
